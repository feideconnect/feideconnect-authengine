{% set pagetitle = "selectprovider" %}
{% extends "base.twig" %}

{% block scripts %}
 <script>
  var require = {
  "paths": {
  "jquery": "../components/jquery/dist/jquery.min",
  "bootstrap": "../components/bootstrap/dist/js/bootstrap.min",
  "tooltip": "../components/bootstrap/js/tooltip"
  },
  "shim": {
  "bootstrap": ["jquery"],
  "tooltip": ["jquery"]
  },
  "urlArgs": "bust={{cacheBust}}"
  };
 </script>

 <script>
  var acrequest = {{requestJSON|raw}};
  window.activeAccounts = {{activeAccountsJSON|raw}};
  window.loc = {{locationJSON|raw}};
  window.config = {{configuration|raw}};
  window.dictionary = {{dictionary|raw}};
  window.client = {{client|raw}};
 </script>

 <script data-main="static/accountchooser/main" src="/static/components/requirejs/require.js"></script>

{% endblock scripts %}



{% block content %}

  <div class="pure-u-1 pure-u-md-2-3 ">
   <h1>{{ pagetitle|trans }}</h1>
   <div class="upper">
    {%- if clientobj.logo %}

    <span class="clientlogo not-on-mobile"><img src="data:image/png;base64,{{ clientobj.logo|b64enc }}" /></span>
    {%- endif %}

    {{ 'tologin'|trans({'{CLIENT}': clientobj.name}) }}
    <div class="clearfix"></div>
   </div>
   <div class="main box">
    <p>This is the accountchooser.twig</p>
   </div>
  </div>

  <div class="pure-u-1 pure-u-md-1-3">
   <div class="box">
    <p>{{ 'whyremember'|trans }}</p>
    <label for="rememberme">
     <input id="rememberme" type="checkbox" checked="checked">
     {% trans %}rememberme{% endtrans %}
    </label>
    <p><small><em>{{ 'sharedcomputer'|trans }}</em></small></p>
   </div>
   <p>{% trans %}alternatechoice{% endtrans %}</p>
   <div class="box">

   </div>
  </div>
{% endblock %}
