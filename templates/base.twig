<!doctype html>
<html lang="{{ currentLanguage }}">
 <head>
  <meta charset="utf-8">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <meta http-equiv="X-UA-COMPATIBLE" content="IE=edge">
  <meta name="robots" content="noindex, nofollow">
  <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, user-scalable=yes">
  <title>{{ pagetitle|trans }}</title>

 {% if isDevelopment == 'dev' %}
  <link href="/static/css/src/style.css" rel="stylesheet" />
 {% else %}
  <link rel="stylesheet" type="tex/css" href="/static/css/style.min.css" />
 {% endif %}

 {# For extra header elements #}
 {% block head %}{% endblock %}
 </head>

 <body id="{{ templateName }}">
 {%- include "partials/languages.twig" %}
 {# Main content block #}
 {%- block content %}{% endblock %}

  <footer>
  {% block footer %}{% endblock %}

  </footer>
 </body>
</html>
